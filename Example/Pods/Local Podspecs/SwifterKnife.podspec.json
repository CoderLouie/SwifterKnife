{
  "name": "SwifterKnife",
  "version": "0.1.4",
  "summary": "A handy collection of Swift method and Tools to build project faster and more efficient.",
  "description": "SwifterKnife is a collection of Swift extension method and some tools that often use in develop project, with them you might build project faster and more efficient.",
  "homepage": "https://github.com/CoderLouie/SwifterKnife",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "liyang": "18616562401@163.com"
  },
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5.3",
  "requires_arc": true,
  "source": {
    "git": "https://github.com/CoderLouie/SwifterKnife.git",
    "tag": "0.1.4"
  },
  "source_files": [
    "SwifterKnife/Classes/Base/**/*",
    "SwifterKnife/Classes/Extension/**/*",
    "SwifterKnife/Classes/Utility/**/*",
    "SwifterKnife/Classes/Views/**/*",
    "SwifterKnife/Classes/PagingView/**/*"
  ],
  "subspecs": [
    {
      "name": "Base",
      "source_files": "SwifterKnife/Classes/Base/*.swift"
    },
    {
      "name": "Extension",
      "source_files": [
        "SwifterKnife/Classes/Base/*.swift",
        "SwifterKnife/Classes/Extension/**/*.swift"
      ]
    },
    {
      "name": "Utility",
      "source_files": "SwifterKnife/Classes/Utility/**/*"
    },
    {
      "name": "Views",
      "source_files": [
        "SwifterKnife/Classes/Base/*.swift",
        "SwifterKnife/Classes/Views/**/*.{swift,h,m}"
      ],
      "exclude_files": "SwifterKnife/Classes/Views/**/*.{h,m}",
      "private_header_files": "SwifterKnife/Classes/Views/**/*.{h}",
      "dependencies": {
        "SnapKit": [

        ]
      }
    },
    {
      "name": "PagingView",
      "source_files": [
        "SwifterKnife/Classes/Base/*.swift",
        "SwifterKnife/Classes/PagingView/**/*.swift"
      ]
    }
  ],
  "swift_version": "5.3"
}
